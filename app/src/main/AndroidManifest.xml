<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.test"
    android:installLocation="preferExternal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.user.neraca_1.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.neraca_1.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.user.neraca_1.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.neraca_1.JenisTransaksiActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.user.neraca_1.MainActivity"></activity>
        <activity
            android:name="com.example.user.neraca_1.JurnalTransaksiActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.user.neraca_1.MainActivity"></activity>
        <activity
            android:name="com.example.user.neraca_1.ObjekTransaksiActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.user.neraca_1.JenisTransaksiActivity"></activity>
        <activity android:name="com.example.user.neraca_1.PembelianActivity"></activity>
    </application>

</manifest>